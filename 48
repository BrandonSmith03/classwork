year = int(input())

if year % 12 == 8:
    print('Dragon')
if year % 12 == 9:
    print('snake')
if year % 12 == 10:
    print('horse')
if year % 12 == 11:
    print('sheep')
if year % 12 == 0:
    print('monkey')
if year % 12 == 1:
    print('rooster')
if year % 12 == 2:
    print('dog')
if year % 12 == 3:
    print('pig')
if year % 12 == 4:
    print('rat')
if year % 12 == 5:
    print('ox')
if year % 12 == 6:
    print('tiger')
if year % 12 == 7:
    print('hare')
